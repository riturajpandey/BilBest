<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:d="http://xamarin.com/schemas/2014/forms/design"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d"
             x:Class="BilBest.Views.Accounts.LoginPage"
             xmlns:controls="clr-namespace:BilBest.CustomControls">
    <ContentPage.Content>
        <StackLayout>
            <!--Main Grid-->
            <Grid HorizontalOptions="FillAndExpand" VerticalOptions="FillAndExpand" Padding="0,0,0,70"
                  BackgroundColor="{StaticResource AppOrangeColor}">
                <controls:CustomFrame HorizontalOptions="FillAndExpand" VerticalOptions="FillAndExpand" BackgroundColor="{StaticResource AppBlueColor}">

                </controls:CustomFrame>
            </Grid>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>