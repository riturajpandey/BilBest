                <On Platform="iOS" Value="Poppins-Black"/>
                <On Platform="Android" Value="Poppins-Black.ttf#Poppins-Black"/>
            </OnPlatform>
            <OnPlatform x:TypeArguments="x:String" x:Key="PoppinsBlackItalic">
